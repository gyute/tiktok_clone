    "@loginTitle": {
      "description": "The title people seel when they open the log in screen.",
      "placeholders": {
        "nameOfTheApp": {
          "type": "String",
          "example": "TikTok"
        }
      }
    },
  "emailPasswordButton": "Use email & password",
  "appleButton": "Continue with Apple",
  "alreadyHaveAnAccount": "Already have an account?",
  "login": "Log in {gender, select, male{sir} female{madam} other{human}}",
  "likeCount": "{value}",
  "@likeCount": {
    "decoration": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentCount": "{value}",
  "@commentCount": {
    "decoration": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentTitle": "{value} {value2, plural, =1{comment} other{comments}}",
  "@commentTitle": {
    "decoration": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  }
