{
  "signUpTitle": "Sign up for {nameOfTheApp}\n{when}",
    "@signUpTitle": {
      "description": "The title people seel when they open the app for the first time.",
      "placeholders": {
        "nameOfTheApp": {
          "type": "String",
          "example": "TikTok"
        },
        "when": {
          "type": "DateTime",
          "format": "M / d",
          "isCustomDateFormat": "true"
        }
      }
    },
